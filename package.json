{
  "name": "imgkit",
  "version": "2.1.0",
  "author": "Aissam Irhir <aissamirhir@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nexus-aissam/imgkit.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "devDependencies": {
    "@napi-rs/cli": "^3.5.1",
    "@types/bun": "latest",
    "sharp": "^0.34.5",
    "tsup": "^8.4.0",
    "typescript": "^5.9.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/nexus-aissam/imgkit/issues"
  },
  "description": "High-performance image processing for Bun and Node.js - resize, convert, compress with HEIC, WebP, AVIF, PNG, JPEG support",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "imgkit-build": "./dist/bin/imgkit-build.js"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://nexus-aissam.github.io/imgkit/",
  "keywords": [
    "image",
    "resize",
    "compress",
    "heic",
    "heif",
    "webp",
    "avif",
    "png",
    "jpeg",
    "jpg",
    "gif",
    "iphone",
    "thumbnail",
    "crop",
    "convert",
    "processing",
    "bun",
    "nodejs",
    "rust",
    "napi",
    "native",
    "fast",
    "turbo"
  ],
  "license": "MIT",
  "optionalDependencies": {
    "imgkit-darwin-arm64": "^2.1.0",
    "imgkit-darwin-x64": "^2.1.0",
    "imgkit-linux-arm64-gnu": "^2.1.0",
    "imgkit-linux-x64-gnu": "^2.1.0",
    "imgkit-linux-x64-musl": "^2.1.0",
    "imgkit-windows-arm64": "^2.1.0",
    "imgkit-windows-x64": "^2.1.0"
  },
  "napi": {
    "binaryName": "image-turbo",
    "packageName": "imgkit",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl"
    ]
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "build:ts": "tsup src/index.ts src/bin/imgkit-build.ts --format cjs,esm --dts --clean",
    "build:all": "bun run build && bun run build:ts",
    "artifacts": "napi artifacts",
    "version": "napi version",
    "test": "bun test test/local",
    "test:local": "bun test test/local",
    "test:bun": "cd test/packages/bun && bun install && bun test",
    "test:npm": "cd test/packages/npm && npm install && npm test",
    "test:yarn": "cd test/packages/yarn && yarn install && yarn test",
    "test:pnpm": "cd test/packages/pnpm && pnpm install && pnpm test",
    "test:packages": "bun run test:bun && bun run test:npm && bun run test:yarn && bun run test:pnpm",
    "test:all": "bun run test:local && bun run test:packages",
    "bench": "bun run benchmarks/bench.ts",
    "lint": "eslint src",
    "clean": "rm -rf dist *.node npm"
  },
  "types": "./dist/index.d.ts"
}
